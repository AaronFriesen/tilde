#!/bin/bash

for d in *; do
    if [ "$d" != "temp" ]; then
        s="$d/Submission_attachment(s)"
        if [ ! -d temp/"$s" ]; then
            mkdir -p "temp/$s"
        fi
        for f in "$s"/*.java; do
            java -cp .:/Users/aaron/bin/res/ Scrubber "$f" >> temp/"$f"
            mv temp/"$f" "$f"
        done
    fi
done
